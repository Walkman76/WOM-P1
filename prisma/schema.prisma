generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id         Int       @id(map: "PK_notes") @default(autoincrement())
  created_at DateTime? @default(dbgenerated("(now())::timestamp(0) without time zone")) @db.Timestamp(6)
  updated_at DateTime? @default(dbgenerated("(now())::timestamp(0) without time zone")) @db.Timestamp(6)
  password   String    @db.VarChar
  username   String?   @unique(map: "unique_username") @db.VarChar

  @@map("users")
}
